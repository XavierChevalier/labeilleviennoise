{
  "name": "labeilleviennoise",
  "version": "0.10.0",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "prepare": "husky install && pnpm build:css",
    "build": "pnpm build:css && remix build",
    "build:css": "pnpm build:css:prepare -m",
    "build:css:prepare": "tailwindcss -i ./app/assets/styles/app.css -o app/assets/styles/app.generated.css",
    "dev": "concurrently -n css,remix \"pnpm dev:css\" \"remix dev\"",
    "dev:css": "pnpm build:css:prepare -w",
    "start": "cross-env NODE_ENV=production netlify dev",
    "lint": "eslint \"**/*.{ts,tsx,js,jsx,mjs,cjs}\"",
    "lint:fix": "pnpm lint --fix",
    "test": "pnpm test:unit",
    "test:unit": "jest"
  },
  "dependencies": {
    "@netlify/functions": "^1.3.0",
    "@remix-run/netlify": "^1.7.3",
    "@remix-run/node": "^1.7.3",
    "@remix-run/react": "^1.7.3",
    "cross-env": "^7.0.3",
    "domain-functions": "^1.3.0",
    "imagekitio-react": "^2.0.0",
    "nodemailer": "^6.8.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.38.0",
    "remix-forms": "^0.19.0",
    "zod": "^3.19.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@remix-run/dev": "^1.7.3",
    "@remix-run/eslint-config": "^1.7.3",
    "@remix-run/serve": "^1.7.3",
    "@tailwindcss/typography": "^0.5.7",
    "@types/nodemailer": "^6.4.6",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "@typescript-eslint/parser": "^5.40.1",
    "concurrently": "^7.4.0",
    "eslint": "^8.25.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "flowbite": "^1.5.3",
    "flowbite-react": "^0.3.4",
    "husky": "^8.0.1",
    "jest": "^29.2.1",
    "jest-extended": "^3.1.0",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "tailwindcss": "^3.2.0",
    "ts-jest": "^29.0.3",
    "typescript": "^4.8.4"
  },
  "lint-staged": {
    "*.{ts,tsx,js,mjs,cjs}": "pnpm eslint --cache --fix",
    "*": "prettier -w -u"
  },
  "engines": {
    "node": ">=14"
  }
}