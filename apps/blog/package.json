{
  "name": "blog.labeilleviennoise.com",
  "description": "Blog of l'Abeille Viennoise",
  "version": "0.0.0",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "prepare": "concurrently -n css,prisma \"pnpm build:css\" \"npx prisma generate\"",
    "build": "pnpm build:css && remix build",
    "build:css": "pnpm build:css:prepare -m",
    "build:css:prepare": "tailwindcss -i ./app/assets/styles/app.css -o app/assets/styles/app.generated.css",
    "dev": "concurrently -c auto \"pnpm:dev:css\" \"pnpm:dev:remix\"",
    "dev:css": "pnpm build:css:prepare -w",
    "dev:remix": "cross-env PORT=3002 remix dev",
    "start": "cross-env NODE_ENV=production netlify dev",
    "test": "pnpm test:unit",
    "test:unit": "jest"
  },
  "dependencies": {
    "@labeilleviennoise/auth-client": "workspace:*",
    "@labeilleviennoise/auth-server": "workspace:*",
    "@labeilleviennoise/catch-boundary": "workspace:*",
    "@labeilleviennoise/environment-server": "workspace:*",
    "@labeilleviennoise/forms-client": "workspace:*",
    "@labeilleviennoise/forms-server": "workspace:*",
    "@labeilleviennoise/icons": "workspace:*",
    "@labeilleviennoise/identifier": "workspace:*",
    "@labeilleviennoise/layouts": "workspace:*",
    "@labeilleviennoise/monitoring": "workspace:*",
    "@labeilleviennoise/router": "workspace:*",
    "@labeilleviennoise/seo": "workspace:*",
    "@labeilleviennoise/ui": "workspace:*",
    "@netlify/functions": "^1.3.0",
    "@prisma/client": "^4.8.0",
    "@remix-run/netlify": "^1.9.0",
    "@remix-run/node": "^1.9.0",
    "@remix-run/react": "^1.9.0",
    "domain-functions": "^1.5.1",
    "imagekitio-react": "^2.0.0",
    "nodemailer": "^6.8.0",
    "react-hook-form": "^7.41.4",
    "remix-forms": "^1.4.0",
    "remix-typedjson": "^0.1.6",
    "showdown": "^2.1.0",
    "tailwind-merge": "^1.8.1",
    "tiny-invariant": "^1.3.1",
    "zod": "^3.20.2"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.9.0",
    "@remix-run/serve": "^1.9.0",
    "@remix-run/server-runtime": "^1.9.0",
    "@tailwindcss/typography": "^0.5.8",
    "@types/jest": "^29.2.5",
    "@types/node": "^18.11.18",
    "@types/nodemailer": "^6.4.7",
    "@types/showdown": "^2.0.0",
    "concurrently": "^7.6.0",
    "cross-env": "^7.0.3",
    "flowbite": "^1.6.0",
    "flowbite-react": "^0.3.7",
    "jest": "^29.3.1",
    "jest-extended": "^3.2.1",
    "prisma": "^4.8.0",
    "tailwindcss": "^3.2.4",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  },
  "engines": {
    "node": ">=14"
  }
}
